{
  "name": "plugin-global",
  "version": "1.0.0",
  "description": "",
  "main": "./src/index.tsx",
  "scripts": {
    "clean:cmjs": "rm -rf dist/cmjs && mkdir dist/cmjs",
    "clean:es6": "rm -rf dist/es6 && mkdir dist/es6",
    "lib": "npm run clean && babel src --out-dir lib --copy-files",
    "build:cmjs": "yarn clean:cmjs && tsc -P tsconfig.json",
    "build:es6": "yarn clean:es6 && tsc -P tsconfig.json --module ES6 --outDir dist/es6",
    "build": "yarn run build:cmjs; yarn run build:es6",
    "watch": "concurrently \"tsc -W\" \"less-watch-compiler src dist\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@antv/g2": "^3.4.9",
    "@antv/g6": "^2.2.6",
    "@antv/g6-editor": "^1.2.0",
    "@types/antd": "^1.0.0",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "antd": "^3.15.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^4.1.0",
    "less-watch-compiler": "^1.13.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-lifecycles-compat": "^3.0.4",
    "react-perfect-scrollbar": "^1.4.4",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "typescript": "^3.3.0-dev.20190108"
  }
}
